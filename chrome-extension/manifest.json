{
  "action": {
    "default_popup": "popup.html",
    "default_width": 400,
    "default_height": 600
  },
  "permissions": ["storage", "scripting", "tabs", "notifications", "webNavigation", "background", "offscreen"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; frame-src 'self'"
  }
}
